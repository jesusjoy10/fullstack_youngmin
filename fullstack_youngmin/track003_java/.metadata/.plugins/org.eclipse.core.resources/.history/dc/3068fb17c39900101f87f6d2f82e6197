package com.company.java010_bank;

//1. 클래스는 부품객체
//2. 상태와 행위
public class Deposit {
       //상태 : 멤버변수
	    UserInfo userinfo;

		public Deposit() { super();  }

		public Deposit(UserInfo userinfo) { super(); this.userinfo = userinfo; }
	    
		//행위 : 멤버함수
		public void exex() {
			//setter / getter 이용해서 사용자에게 입금받기 ### 
		}
	    
}
/* 기능: 사용자에게 입금받기 
*/