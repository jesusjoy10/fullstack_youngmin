package com.company.java008;

public class Method004 {
	//오버로딩? 같은목적으로 비슷한 동작을 수행하는 매서드들의 [이름을 일관]되게 작업
	//       [파라미터의 타입]과 [갯수]로 매서드 구분
	public static void show(){System.out.println("마법상자");}
	
	public static void show(int a) {System.out.println("무조건*10>" +a*10);}
	
	////////////////////////////////////////
	public static void main(String[]args) {
		int[] arr= {1,2,3}; //3:0~2
		//public static 리턴값 매서드명(파라미터){해야할일}
		show();//public static void show(){}
		show(1); //public static void show(int a){10배}   10배
		show(arr[1]); // show(2) 
	}
	////////////////////////////////////////	
}
