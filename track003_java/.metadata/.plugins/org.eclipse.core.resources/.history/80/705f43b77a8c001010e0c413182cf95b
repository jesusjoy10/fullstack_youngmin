package com.company.java005_ex;

public class ForEx004 {
	public static void main(String[]args) {
		int cnt = 0;  // 1~10까지의 3의 배수의 갯수 카운트를 담을 변수
		
		
		for (int i=1; i<=10; i++ ) {if(i % 9 ==0){ cnt++;}}
		
		//출력
		System.out.println("1~10까지 3의 배수 갯수: " + cnt);
		
		
	}

}


/* 출력내용 :   for 이용
1~10까지 3의 배수 갯수를 출력   

upgrade)  시간나면 도전!
3의배수 : 3,6,9    
갯수 : 3개
*/