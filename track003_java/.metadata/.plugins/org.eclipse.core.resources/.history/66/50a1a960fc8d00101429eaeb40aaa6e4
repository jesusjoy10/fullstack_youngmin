package com.company.java005_ex;

public class RepeatEx010 {
	public static void main(String[] args) {

		int count = 0;
		System.out.println("for");
		for (char a = 'A'; a <= 'Z'; a++) {
			System.out.print(a + "\t");
			count++;
			if (count == 5) {
				System.out.println();
				count = 0;
			}

		}

	}

}


/*for , while , do while 3가지 버젼으로 
ABCDE   
FGHIJ
KLMNO
PQRST
UVWXY
Z 
*/