package com.company.java006;

import java.util.Arrays;

public class Ar002 {
    public static void main(String[] args) {
        int[] arr = {1, 2, 3};
        int[] arr2 = new int[3];

        System.out.println(arr2); // 주소 출력됨
        System.out.println(Arrays.toString(arr2)); // [0, 0, 0]
    }
}

